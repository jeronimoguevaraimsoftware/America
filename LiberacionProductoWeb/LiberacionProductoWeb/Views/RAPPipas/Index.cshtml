                                    <div class="panel-body">
                                        <table id="data-table" class="table table-bordered table-hover">
                                            <thead>
                                                <tr>
                                                    <th rowspan="3" class="headcol">Fecha</th>
                                                    <th rowspan="3" class="headcol">Hora</th>
                                                    <th rowspan="3" class="headcol">No. de lote de Producción</th>
                                                    <th rowspan="3" class="headcol">No. de lote de Distribución</th>
                                                    <th rowspan="3" class="headcol">
                                                        Fecha de caducidad del lote de producción
                                                        (Acondicionamiento)
                                                    </th>
                                                    <th colspan="@(maxInitial + maxFinal + 2)">
                                                        Control del proceso de acondicionamiento
                                                    </th>
                                                    <th colspan="8">Aseguramiento de acondicionamiento</th>
                                                    <th rowspan="3">Observaciones</th>
                                                </tr>
                                                <tr>
                                                    <th colspan="@(maxInitial + 1)">Análisis inicial</th>
                                                    <th colspan="@(maxFinal + 1)">Análisis final</th>
                                                    <th rowspan="2">Folio del informe de desviación</th>
                                                    <th rowspan="2">Folio del trabajo no conforme</th>
                                                    <th colspan="2">Producto no conforme</th>
                                                    <th rowspan="2">Folio del evento adverso</th>
                                                    <th rowspan="2">No. de devolución</th>
                                                    <th rowspan="2">Folio de retiro de productos</th>
                                                    <th rowspan="2">Folio de control de cambios</th>
                                                </tr>
                                                <tr>
                                                    @foreach (var initialAnalysisEntry in Model.ListRAPPipas.Where(x => x.aInicial.Count == maxInitial).Select(x => x.aInicial).FirstOrDefault())
                                                    {
                                                        <th>@initialAnalysisEntry.ParameterName / @initialAnalysisEntry.Specification</th>
                                                    }
                                                    <th>Aprobado Sí/No</th>
                                                    @foreach (var finalAnalysisEntry in Model.ListRAPPipas.Where(x => x.aFinal.Count == maxFinal).Select(x => x.aFinal).FirstOrDefault())
                                                    {
                                                        <th>@finalAnalysisEntry.ParameterName / @finalAnalysisEntry.Specification</th>
                                                    }
                                                    <th>Aprobado Sí/No</th>
                                                    <th scope='col'>Folio de PNC</th>
                                                    <th scope='col'>Disposición de PNC</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var entry in Model.ListRAPPipas)
                                                {
                                                    <tr>
                                                        <td>@entry.Fecha.Value.ToString("yyyy/MM/dd")</td>
                                                        <td>@entry.Hora</td>
                                                        <td><a href="/ProductionOrder/New?Id=@entry.ProductionOrderId">@entry.NumeroLoteProduccion</a></td>
                                                        <td>
                                                            <a href="/ConditioningOrder/New?IdOP=@entry.ProductionOrderId">@entry.NumeroLoteDistribucion</a>
                                                        </td>
                                                        <td>@entry.FechaCaducidad.Value.ToString("yyyy/MM/dd")</td>
                                                        @for (int i = 0; i < maxInitial; i++)
                                                        {
                                                            if (i < entry.aInicial.Count)
                                                            {
                                                                <td>@entry.aInicial[i].Value</td>
                                                            }
                                                            else
                                                            {
                                                                <td></td>
                                                            }
                                                        }
                                                        <td>@entry.Aprobado</td>
                                                        @for (int i = 0; i < maxFinal; i++)
                                                        {
                                                            if (i < entry.aFinal.Count)
                                                            {
                                                                <td>@entry.aFinal[i].Value</td>
                                                            }
                                                            else
                                                            {
                                                                <td></td>
                                                            }
                                                        }
                                                        <td>@entry.Aprobado</td>
                                                        <td>@entry.aseguramiento.FolioInformeDesviacion</td>
                                                        <td>@entry.aseguramiento.FolioTrabajoNoCOnforme</td>
                                                        <td>@entry.aseguramiento.FolioPNC</td>
                                                        <td>@entry.aseguramiento.DisposicionPNC</td>
                                                        <td>@entry.aseguramiento.FolioEventoAdverso</td>
                                                        <td>@entry.aseguramiento.NumeroDeDevolucion</td>
                                                        <td>@entry.aseguramiento.FolioRetiroProductos</td>
                                                        <td>@entry.aseguramiento.FolioDeControlDeCambios</td>
                                                        <td>@entry.Observaciones</td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="section-container section-with-top-border p-b-5" style="width: 30%; float:left">
                            <!-- Mensaje de error -->
                            <div class="col-md-12" id="message"></div>
                            <div class="panel-heading">

