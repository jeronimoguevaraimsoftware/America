                                        </tr>
                                        <tr>
                                            <th>Total</th>
                                            <th>@Model.Total</th>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>

                        <div class="section-container section-with-top-border p-b-5" style="width: 70%; float:right">
                            <a href="javascript:;" class="btn btn-white m-r-5" id="ExporttoExcelv">
                                <i class="fa fa-file-excel"></i> Exportar a excel
                            </a>
                            <br />
                            <!-- Mensaje de error -->
                            <div class="col-md-12" id="message"></div>
                            <div class="panel-body">
                                <table id="data-table-variables" class="table table-bordered table-hover" style="width: 100%; float:left">
                                    <thead>
                                        <tr>
                                            <th>
                                                Etapa
                                            </th>
                                            <th>
                                                Parámetro
                                            </th>
                                            <th>
                                                Especificación
                                            </th>
                                            <th>
                                                Límite superior de control
                                            </th>
                                            <th>
                                                Límite inferior de control
                                            </th>
                                            <th>
                                                Valor máximo
                                            </th>
                                            <th>
                                                Valor promedio
                                            </th>
                                            <th>
                                                Valor mínimo
                                            </th>
                                            <th>
                                                Desviación estándar
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.ListAnalisisInitial != null)
                                        {
                                            @foreach (var entry in Model.ListAnalisisInitial)
                                            {
                                                <tr>
                                                    <th>Análisis inicial</th>
                                                    <th>@entry.Parametro</th>
                                                    <th>@entry.Especificacion</th>
                                                    <th>@entry.LSC</th>
                                                    <th>@entry.LIC</th>
                                                    <th>@entry.ValorMax</th>
                                                    <th>@entry.ValorProm</th>
                                                    <th>@entry.ValorMin</th>
                                                    <th>@entry.DesvEstandar</th>
                                                </tr>
                                            }
                                        }

                                        @if (Model.ListAnalisisFinal != null)
                                        {
                                            @foreach (var entry in Model.ListAnalisisFinal)
                                            {
                                                <tr>
                                                    <th>Análisis final</th>
                                                    <th>@entry.Parametro</th>
                                                    <th>@entry.Especificacion</th>
                                                    <th>@entry.LSC</th>
                                                    <th>@entry.LIC</th>
                                                    <th>@entry.ValorMax</th>
                                                    <th>@entry.ValorProm</th>
                                                    <th>@entry.ValorMin</th>
                                                    <th>@entry.DesvEstandar</th>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    }
                    else
                    {
                        <div class="text-center">
                            <div class="text-center">
